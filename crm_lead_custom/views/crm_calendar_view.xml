<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_crm_calendar_crm_leads_year" model="ir.ui.view">
            <field name="name">inherit_crm_calendar_crm_leads_year</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_case_calendar_view_leads"/>
            <field name="arch" type="xml">
                <xpath expr="//calendar" position="attributes">
                    <attribute name="date_start">date_event</attribute>
                    <attribute name="color">color</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>